# Configuration de la base de données MySQL
DB_HOST=localhost
DB_PORT=3306
DB_NAME=feelsame_db
DB_USER=feelsame_user
DB_PASSWORD=your_secure_password

# Configuration JWT
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production
